
@{
    Layout = null;
}

@model ImageUploadModel
<form asp-action="UploadImage" method="post" enctype="multipart/form-data" id="img-form" onsubmit="return jQueryAjaxPost(this)">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label for="exampleFormControlFile1">Profil Resmi Seçin</label>
        <input asp-for="file" class="form-control-file" />
    </div>
    <button type="submit" class="btn btn-first">Yükle</button>
</form>
