
@{
    Layout = null;
}


<form asp-action="AddTag" method="post" onsubmit="return jQueryAjaxPost(this)">
    <div asp-validation-summary="All"></div>
    <div class="card">
        <div class="card-body">
            <div class="form-group">
                <label for="squareInput">Etiket Adı</label>
                <input type="text" class="form-control input-square" name="tagname" id="tagname" placeholder="Etiket Giriniz..">
            </div>

            <div class="card-action">
                <button class="btn btn-success">Ekle</button>
                <button class="btn btn-danger" data-dismiss="modal">İptal</button>
            </div>
        </div>
    </div>
</form>